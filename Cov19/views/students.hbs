<body>
    <div class="allcards">
        {{#each student}}
        <div class="card" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">{{this.Name}}</h5>
                <p class="card-text"><b>{{this.Email}}</b> {{this.Age}}</p>
                <a href="#" class="btn btn-primary">{{this.Number}}</a>
            </div>
        </div>  
        {{/each}} 
    </div>
</body>